                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct
dc.idCliente,
dpr.idProducto,
dpv.idProveedor,
de.idEmpleado,
dce.idCompEnvio,
dt.idTiempo,
round(pd.Cantidad*pd.PrecioUnidad*(1-pd.Descuento), 2) as mventa,
pd.Cantidad as cantidad,
pd.Descuento as descuento
from Neptuno.dbo.Clientes					c 
inner join Neptuno.dbo.Pedidos				p	on c.IdCliente			=	p.IdCliente
inner join Neptuno.dbo.DetallesPedidos		pd	on pd.IdPedido			=	p.IdPedido
inner join Neptuno.dbo.Productos			pr	on pd.IdProducto		=	pr.IdProducto
inner join Neptuno.dbo.Proveedores			pv	on pr.IdProveedor		=	pv.IdProveedor
inner join Neptuno.dbo.Empleados			e	on e.IdEmpleado			=	p.IdEmpleado
inner join Neptuno.dbo.CompaniasEnvios		ce	on p.FormaEnvío			=	ce.IdCompañíaEnvíos
inner join nephdm.dbo.dimCliente			dc	on dc.nombreCompania	=	c.NombreCompañía			collate Modern_Spanish_CI_AS
inner join nephdm.dbo.dimProducto			dpr	on dpr.nomProducto		=	pr.NombreProducto			collate Modern_Spanish_CI_AS
inner join nephdm.dbo.dimProveedor			dpv on dpv.nomProveedor		=	pv.NombreCompañía			collate Modern_Spanish_CI_AS
inner join nephdm.dbo.dimEmpleado			de	on de.nomEmpleado		=	e.Apellidos+' '+e.Nombre	collate Modern_Spanish_CI_AS
inner join nephdm.dbo.dimCompEnvio			dce on dce.nomCompEnvio		=	ce.NombreCompañía			collate Modern_Spanish_CI_AS
inner join nephdm.dbo.dimTiempo				dt	on dt.fecha				=	p.FechaPedido</property>
